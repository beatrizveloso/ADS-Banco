CREATE TABLE tb_funcionário{
    id_func_pk INT PRIMARY KEY,
    nome VARCHAR(30),
    salario FLOAT, 
    idade INT,
    departamento_fk INT,
    CONSTRAINT dep_fk FOREING KEY(departamento_fk),
    REFERENCES tb_departamento(id_dep_pk)
}

